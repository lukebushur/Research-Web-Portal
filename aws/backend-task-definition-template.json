{
  "family": "back-end",
  "containerDefinitions": [
    {
      "name": "back-end",
      "image": "<SERVER_IMAGE_TAG>",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "name": "server",
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "ACCESS_TOKEN_EXPIRY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:ACCESS_TOKEN_EXPIRY::"
        },
        {
          "name": "ADMIN",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:ADMIN::"
        },
        {
          "name": "DB_DEV_COLLECTION",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:DB_DEV_COLLECTION::"
        },
        {
          "name": "DB_UNIT_TEST_COLLECTION",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:DB_UNIT_TEST_COLLECTION::"
        },
        {
          "name": "DB_URI",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:DB_URI::"
        },
        {
          "name": "EMAIL_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:EMAIL_PASSWORD::"
        },
        {
          "name": "EMAIL_USER",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:EMAIL_USER::"
        },
        {
          "name": "FACULTY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:FACULTY::"
        },
        {
          "name": "FRONT_END_IP",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:FRONT_END_IP::"
        },
        {
          "name": "INDUSTRY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:INDUSTRY::"
        },
        {
          "name": "PORT",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:PORT::"
        },
        {
          "name": "REFRESH_TOKEN_EXPIRY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:REFRESH_TOKEN_EXPIRY::"
        },
        {
          "name": "SECRET_ACCESS_TOKEN",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:SECRET_ACCESS_TOKEN::"
        },
        {
          "name": "SECRET_REFRESH_TOKEN",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:SECRET_REFRESH_TOKEN::"
        },
        {
          "name": "STUDENT",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:STUDENT::"
        },
        {
          "name": "UNITTESTEMAIL",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:UNITTESTEMAIL::"
        },
        {
          "name": "UNITTESTPASS",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:603246038799:secret:rwp/dev-jtdkOA:UNITTESTPASS::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/back-end",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "tags": [
    {
      "key": "billingResource",
      "value": "ECS Back-End Task"
    }
  ],
  "taskRoleArn": "arn:aws:iam::603246038799:role/ECSBackEndTaskSecretManager",
  "executionRoleArn": "arn:aws:iam::603246038799:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "ephemeralStorage": {
    "sizeInGiB": 21
  },
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false
}
